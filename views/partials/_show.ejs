<% for(var i = 0; i < users.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= users[i].name %></td>
        <td><%= users[i].email %></td>
        <td><%= users[i].number %></td>
        <td>
            <a href="/update-contact?id=<%= users[i]._id%>" class="btn border-shadow update">
                <span class="standardcolor"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
                <span class="standardcolor"><i class="fas fa-trash"></i></span>
            </a>
        </td>
    </tr>
<% } %>